package progs;

public class TestIT {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		//Number palindrome
		/*
		create three int variable (reminder,temp,sum) initialize one with zero(sum)
		create another int variable(number) and assign the value 
		assign that number value to temp 
		put while loop as number > 0
		get number%10 and assign to reminder varriable (i.e reminder=number%10)
		multiple sum with 10 and append the reminder and assign to sum itself
		get the reminder for number by 10
		put if condition temp equal to sum then palindrome else not palindrome.
		*/
		
		int reminder, temp, sum=0;
		int number = 145;
		temp = number;
		while(number>0) {
			reminder = number %10;
			reminder +=	sum*10;
			
		}

	}
}
