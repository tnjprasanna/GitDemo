package progs;

public class TestIT {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		//Prime number
		/*
		create int varriable as number and assign the value
		create two int varriable and initialize with zero (m, flag)
		divide the number by 2 and assign the quotient to m varriable
		put if condition number is equal to 0 or 1 then its not a prime number
		else block put for loop i starts with 2 and condition check i <= m and i++
		put if condition number%i == 0 then assign flag value as 1 then break the if loop
		in else block outside the for loop check if flag equal to 0 then print as prime number
		outside the else block print not a prime number
		*/
		
		int number = 123;
		int m = 0, flag = 0;
		m = number/2;
		if(number == 0 || number == 1) {
			System.out.println("not a prime");
		} else {
			for(int i=2;i<=m;i++) {
				if(number%i == 0) {
					flag =1;
					break;
				}
			}
			if(flag==0) {
				System.out.println("its a prime");
			}
		}
		System.out.println("its not a prime");
	}
}
