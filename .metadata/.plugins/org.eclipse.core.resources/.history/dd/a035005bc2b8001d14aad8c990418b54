package product;

public class Product {
	
	String product;
	int price;
	
	public Product() {
		super();
		// TODO Auto-generated constructor stub
	}
	
	
	public String getProduct() {
		return product;
	}
	public void setProduct(String product) {
		this.product = product;
	}
	public int getPrice() {
		return price;
	}
	public void setPrice(int price) {
		this.price = price;
	}

}
