package progs;

public class TestIT {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		//Duplicate String in array
		/*
		Create one static string array varriable
		assign the few values to that string array
		find the array length and store to int varriable
		create two nested for loop with int i and j
		start i with zero and equal to length
		start j with i + 1 and equal to length
		Put if condition arr[i] equalignorecase to arr[j] and i!=j
		print arr[j]
		*/
		
		String arr[] = {"d","f","d","a"};
		int arrLen = arr.length;
		for(int i=0;i<=arrLen;i++) {
			for(int j=i+1;j<=arrLen;j++) {
				if(arr[i].equalsIgnoreCase(arr[j]) & i!=j) {
					System.out.println(arr[j]);
				}
			}
		}
		
	}
}
